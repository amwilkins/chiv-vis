{
	"compilerOptions": {
		"module": "commonjs",
		"esModuleInterop": true,
		"resolveJsonModule": true,
		"target": "es6",
		"noImplicitAny": false,
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist",
		"baseUrl": ".",
		"paths": {
			"*": [
				"node_modules/*"
			]
		}
	},
	"include": [
		"src/**/*"
	],
	"files": [],
	"references": [
		{
			"path": "./tsconfig.app.json"
		},
		{
			"path": "./tsconfig.node.json"
		}
	]
}
